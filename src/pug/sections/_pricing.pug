section.pricing.scrollspy#pricing
    .container
        .row
            .col.s12.center
                h2 Pricing
        .row
            .col.s12.l4.big-margin-on-medium-and-down
                .price-box
                    h4
                        sup $
                        span.price 19
                    .divider
                    ul.flow-text
                        li 5 users
                            span.info.tooltipped(data-position='top', data-delay='50', data-tooltip='The number of allowed users') ?
                        li Automation
                            span.info.tooltipped(data-position='top', data-delay='50', data-tooltip='Involves Gulp commands') ?
                        li Validation
                        li Linting

            .col.s12.l4.big-margin-on-medium-and-down
                .price-box.featured
                    h4
                        sup $
                        span.price 49
                    .divider
                    ul.flow-text
                        li 10 users
                            span.info.tooltipped(data-position='top', data-delay='50', data-tooltip='The number of allowed users') ?
                        li Automation
                            span.info.tooltipped(data-position='top', data-delay='50', data-tooltip='Involves Gulp commands') ?
                        li Validation
                        li Linting
                        li Templating
                            span.info.tooltipped(data-position='top', data-delay='50', data-tooltip='Includes Jade templates engine') ?

            .col.s12.l4
                .price-box
                    h4
                        sup $
                        span.price 199
                    .divider
                    ul.flow-text
                        li 25 users
                            span.info.tooltipped(data-position='top', data-delay='50', data-tooltip='The number of allowed users') ?
                        li Automation
                            span.info.tooltipped(data-position='top', data-delay='50', data-tooltip='Involves Gulp commands') ?
                        li Validation
                        li Linting
                        li Templating
                            span.info.tooltipped(data-position='top', data-delay='50', data-tooltip='Includes Jade templates engine') ?
                        li Preprocessors
                            span.info.tooltipped(data-position='top', data-delay='50', data-tooltip='This is for Jade, Sass and CoffeeScript') ?